type
  RSBitmaps = record(TSRLBaseRecord)
  class var
    CloseInterfaceButton: TMufasaBitmap;
    CloseScrollInterfaceButton: TMufasaBitmap;

    MinimapFlag: TMufasaBitmap;
    ChooseOption: TMufasaBitmap;

    XPBarSmall: TMufasaBitmap;
    XPBarMedium: TMufasaBitmap;
    XPBarLarge: TMufasaBitmap;

    SpellBookRegular: TMufasaBitmap;
    SpellBookLunar: TMufasaBitmap;
    SpellBookArceuus: TMufasaBitmap;
    SpellBookAncient: TMufasaBitmap;
  end;

function RSBitmaps.Load(Width, Height: Integer; Data: String): TMufasaBitmap; static;
begin
  Result := TMufasaBitmap.CreateFromString(Width, Height, Data);
  Result.FreeOnTerminate(True);
end;

